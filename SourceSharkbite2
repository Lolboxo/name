local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SharkBite 2 FreeBoats V1", "Ocean")

--BoatsXMainFrame
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Boats")


Section:NewButton("DuckyBoat", "Chooses DuckyBoat", function()
    local args = {
		[1] = "DuckyBoat"
	}

	game:GetService("ReplicatedStorage").EventsFolder.BoatSelection.UpdateHostBoat:FireServer(unpack(args))
end)


Section:NewButton("BlueWoodenMotorboat", "Chooses BlueWoodenMotorboat", function()
    local args = {
		[1] = "BlueWoodenMotorboat"
